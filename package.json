{
  "name": "RNBoilerplate",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "lint": "node_modules/.bin/eslint app --fix",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "build:ios": "react-native run-ios --configuration Release",
    "build:android": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/"
  },
  "dependencies": {
    "immutability-helper": "^2.2.0",
    "prop-types": "^15.5.10",
    "react": "^16.0.0-alpha.12",
    "react-native": "^0.47.2",
    "react-native-router-flux": "^4.0.0-beta.18",
    "react-native-svg": "^5.4.1",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-persist": "^4.0.0-alpha7",
    "redux-thunk": "^2.1.0"
  },
  "rnpm": {
    "assets": [
      "./app/assets"
    ]
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "babel-preset-react-native": "^2.1.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.2.1",
    "jest": "^20.0.4",
    "mocha": "^3.5.0",
    "react-test-renderer": "^16.0.0-alpha.12"
  },
  "jest": {
    "preset": "react-native",
    "testRegex": "./__tests__/[^setup].*.js$",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|native-base|react-navigation)"
    ],
    "setupFiles": [
      "./__tests__/setup.js"
    ]
  }
}
